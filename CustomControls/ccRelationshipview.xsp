<?xml version="1.0" encoding="UTF-8"?>
<xp:view xmlns:xp="http://www.ibm.com/xsp/core">

	<xp:button value="Delete" id="button1"><xp:eventHandler event="onclick" submit="true" refreshMode="complete">
